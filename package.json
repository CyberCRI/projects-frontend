{
    "name": "lpi-projects",
    "version": "2.1.2",
    "private": true,
    "description": "LPI projects",
    "author": {
        "name": "LPI IT team"
    },
    "scripts": {
        "serve": "vite",
        "build": "vite build",
        "build:report": "trash report && vue-cli-service build",
        "test:unit": "vitest",
        "test:unit:ci": "node --expose-gc --gc_global ./node_modules/.bin/vitest --logHeapUsage",
        "test:gen": "npx playwright codegen --ignore-https-errors http://localhost:8080",
        "test:play": "npx playwright test --project=chrome",
        "test:play:webkit": "npx playwright test --project=webkit",
        "test:play:chromium": "npx playwright test --project=chromium",
        "test:play:edge": "npx playwright test --project=edge",
        "test:play:read": "npx playwright test tests/playwright/read --project=chrome",
        "test:play:write": "npx playwright test tests/playwright/write --project=chrome",
        "test:play:websocket": "npx playwright test tests/playwright/websocket --project=chrome",
        "lint:ci": "yarn run lint:ci:js && yarn run lint:ci:scss && yarn run check-types",
        "lint:ci:js": "node --expose-gc --gc_global ./node_modules/.bin/eslint --ext .js,.vue,.ts --ignore-path .gitignore src",
        "lint:ci:scss": "node --expose-gc --gc_global ./node_modules/.bin/stylelint -c .stylelintrc.json \"**/*.{vue,scss,sass,css}\"",
        "lint": "yarn run lint:js && yarn run lint:scss && yarn run check-types",
        "lint:js": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src",
        "lint:scss": "stylelint \"**/*.{vue,scss,sass,css}\" --fix",
        "check-types": "tsc --noemit",
        "format": "prettier --write \"**/*.{json,js,ts,vue,scss,sass,css}\"",
        "format:check": "prettier --check \"**/*.{json,js,ts,vue,scss,sass,css}\"",
        "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
        "prepare": "husky install",
        "check-unused-comp": "check-unused-comp ."
    },
